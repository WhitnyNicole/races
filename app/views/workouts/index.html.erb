<h1> All Workouts! </h1>

<ul>
  <% @workouts.each do |workout| %>
<li>
  <strong><%= workout.name %></strong> -
  <%= workout.distance %> miles
  <%= workout.pace %> = added by: <%= workout.user.username %>
  <%= link_to "See all Training Sessions", workout_trainings_path(workout) %>
  <%= link_to "Add a Training Session", new_workout_training_path(workout) %>
</li>

<% end %>
</ul>
